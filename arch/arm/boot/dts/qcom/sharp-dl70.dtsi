		pinctrl-1 = <&fel_int_suspend>;
	};

	gp2a_prox {
		compatible = "gp2a-prox";
		qcom,prox-int-gpio  = <&msm_gpio 40 0x00>;
		interrupt-parent  = <&msm_gpio>;

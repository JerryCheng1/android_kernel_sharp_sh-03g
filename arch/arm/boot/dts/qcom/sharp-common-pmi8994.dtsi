/*****************************************/
/*  camera led                           */
/*****************************************/
&spmi_bus{
    qcom,pmi8994@3{
        qcom,leds@d300{
            /delete-property/ qcom,clamp-curr;
            /delete-property/ qcom,thermal-derate-enabled;
            /delete-property/ qcom,thermal-derate-threshold;
            /delete-property/ qcom,thermal-derate-rate;
            /delete-property/ qcom,vph-pwr-droop-enabled;
            /delete-property/ qcom,vph-pwr-droop-threshold;
            /delete-property/ qcom,vph-pwr-droop-debounce-time;
            /delete-property/ qcom,headroom-sense-ch1-enabled;
            /delete-property/ qcom,power-detect-enabled;
        };
    };
};

&pmi8994_flash0{
    qcom,clamp-curr = <100>;
    qcom,duration = <250>;
};

/delete-node/ &pmi8994_flash1;

&pmi8994_torch0{
    qcom,max-current = <1000>;
    qcom,current = <25>;
};

/delete-node/ &pmi8994_torch1;

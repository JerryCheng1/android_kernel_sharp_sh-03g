		qcom,mode = <0>;			/* DIG_IN */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <4>;			/* PULL_DN */
		qcom,vin-sel = <2>;			/* S4 */
		qcom,out-strength = <3>;	/* HIGH */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENSABLE */
		status = "okay";
	};

	gpio@ce00 { /* GPIO_15 */
		/* PM_DIVCLK1 */
		/* Qualcomm Settings */
		qcom,mode = <1>;
		qcom,output-type = <0>;
		qcom,pull = <5>;
		qcom,vin-sel = <2>;
		qcom,out-strength = <1>;
		qcom,src-sel = <2>;
		qcom,master-en = <1>;
		status = "okay";
	};

	gpio@cf00 { /* GPIO_16 */
		/* PMI_CLK */
		/* Qualcomm Settings */
	};

	gpio@d000 { /* GPIO_17 */
		/* (DIVCLK3) HUB_CLK */
		qcom,mode = <0>;			/* DIG_IN */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <4>;			/* PULL_DN */
		qcom,vin-sel = <2>;			/* S4 */
		qcom,out-strength = <3>;	/* HIGH */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENSABLE */
		status = "okay";
	};

	gpio@d100 { /* GPIO_18 */
		/* DIVCLK4 */
		/* Qualcomm Settings */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,invert = <0>;		/* Output low initially */
		qcom,pull = <5>;			/* NO_PULL */
		qcom,vin-sel = <2>;		/* VIN 2 */

/*****************************************/
/*  pm8994: qcom,pm8994 alias            */
/*****************************************/

&spmi_bus {
	qcom,pm8994@0 {
		qcom,power-on@800 {
			qcom,s3-debounce = <16>;
			qcom,s3-src = "kpdpwr";

			qcom,pon_1 {
				qcom,pon-type = <0>;
				qcom,pull-up = <1>;
				linux,code = <116>;
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <1000>;
				qcom,s2-type = <7>;
			};

			qcom,pon_2 {
				qcom,pon-type = <1>;
				qcom,support-reset = <1>;
				qcom,pull-up = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <1000>;
				qcom,s2-type = <7>;
				linux,code = <114>;
				qcom,use-bark;
			};

			qcom,pon_3 {
				qcom,pon-type = <3>;
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <1000>;
				qcom,s2-type = <7>;
				qcom,pull-up = <1>;
				qcom,use-bark;
			};
		};
	};
	
	qcom,pm8994@0 {
		pm8994_vadc: vadc@3100 {
		};
		
		pm8994_adc_tm: vadc@3400 {
		};
	};

	qcom,pm8994@1 {
	};
};


/*****************************************/
/*  pm8994-mpps: qpnp-pin                */
/*****************************************/

&pm8994_mpps {

	mpp@a000 { /* MPP_01 */
		/* SDC_UIM_VBIAS */
		/* Qualcomm Settings */
	};

	mpp@a100 { /* MPP_02 */
		/* Unused */
		qcom,mode = <6>;		/* Current Sink */
		qcom,src-sel = <0>;		/* MPP is always disabled */
		qcom,ain-route = <1>;	/* QPNP_PIN_AIN_AMUX_CH6 */
		status = "okay";
	};

	mpp@a200 { /* MPP_03 */
		/* VREF_DACX */
		/* Qualcomm Settings */
	};

	mpp@a300 { /* MPP_04 */
		/* Unused */
		qcom,mode = <6>;		/* Current Sink */
		qcom,src-sel = <0>;		/* MPP is always disabled */
		qcom,ain-route = <3>;	/* QPNP_PIN_AIN_AMUX_CH8 */
		status = "okay";
	};

	mpp@a400 { /* MPP_05 */
		/* Unused */
		qcom,mode = <6>;		/* Current Sink */
		qcom,src-sel = <0>;		/* MPP is always disabled */
		qcom,ain-route = <0>;	/* QPNP_PIN_AIN_AMUX_CH5 */
		status = "okay";
	};

	mpp@a500 { /* MPP_06 */
		/* LPDDR_VSEL */
		/* Qualcomm Settings */
	};

	mpp@a600 { /* MPP_07 */
		/* AUX_PMIC_PON */
		/* Qualcomm Settings */
	};

	mpp@a700 { /* MPP_08 */
		/* PMI8994_VCHG */
		qcom,mode = <4>;		/* Analog Input */
		qcom,src-sel = <1>;		/* MPP is always enabled */
		qcom,ain-route = <3>;	/* QPNP_PIN_AIN_AMUX_CH8 */
		qcom,master-en = <1>;		/* ENSABLE */
		status = "okay";
	};
};
